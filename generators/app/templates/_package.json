    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "cucumber": "2.0.0-rc.9",
    "cucumber-html-reporter": "0.3.7",
    "enzyme": "2.7.1",
    "eslint": "2.11.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-react": "6.6.0",
    "expect.js": "0.3.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fake-xml-http-request": "1.4.0",
    "file-loader": "0.9.0",
