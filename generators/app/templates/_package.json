{
  "name": "<%=appname%>",
  "version": "0.0.0",
  "description": "TBD",
  "main": "server.js",
  "scripts": {
    "acceptance": "./node_modules/protractor/bin/protractor protractor.acceptance.conf.js",
    "build": "node deployment/scripts/deploy.js",
    "bump-version:commit": "git commit -am 'version bump'",
    "bump-version:major": "chmod u+x ./tasks/bump-version.js & ./tasks/bump-version.js --bump=major",
    "bump-version:minor": "chmod u+x ./tasks/bump-version.js & ./tasks/bump-version.js --bump=minor",
    "bump-version:patch": "chmod u+x ./tasks/bump-version.js & ./tasks/bump-version.js",
    "client": "webpack-dev-server --hot --inline --profile --colors --display-error-details --display-cached & npm run delayed-open & wait",
    "coverage": "babel-node node_modules/babel-istanbul/lib/cli.js cover _mocha ./test/spec.js -- --require ignore-styles --require mocha-clean && npm run lint",
    "coverage-clean": "npm run rimraf -- coverage",
    "coverage-open": "open coverage/index.html",
    "delayed-open": "sleep 5 && opener http://localhost:$npm_package_config_clientPort",
    "docker-client": "webpack-dev-server --host=0.0.0.0 --hot --inline --port 2368 & wait",
    "docker-server": "concurrently \"npm run server\" \"npm run docker-client\"",
    "e2e": "./node_modules/protractor/bin/protractor protractor.conf.js",
    "e2e-test-server": "concurrently --kill-others \"npm run server\" \"webpack-dev-server \"",
    "eb-bundle:beta": "cd deployment && zip -r ./build.zip Dockerrun.aws.json .ebextensions/01_redirect.config .ebextensions/02_beta_env_variables.config",
    "eb-bundle:production": "cd deployment && zip -r ./build.zip Dockerrun.aws.json .ebextensions/01_redirect.config .ebextensions/02_prod_env_variables.config",
    "eb-docker": "chmod u+x ./tasks/eb-docker.js & ./tasks/eb-docker.js --appName toolshed-client --buildNumber $BUILD",
    "eb-tag": "chmod u+x ./tasks/eb-dockerrun-tag.js & ./tasks/eb-dockerrun-tag.js",
    "lint": "eslint src --ext=jsx --ext=js",
    "postacceptance": "node_modules/babel-istanbul/lib/cli.js report --include=coverage/*.json && open coverage/index.html",
    "poste2e": "node_modules/babel-istanbul/lib/cli.js report --include=coverage/*.json && open coverage/index.html",
    "postinstall": "./node_modules/protractor/bin/webdriver-manager update",
    "preacceptance": "npm run coverage-clean",
    "prebuild": "rimraf dist && webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe",
    "precoverage": "npm run coverage-clean",
    "pree2e": "npm run coverage-clean",
    "rimraf": "rimraf",
    "server": "json-server --watch --delay 500 json-server/db.json --id _id --port $npm_package_config_apiPort --routes routes.json",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "test": "mocha ./test/spec.js --compilers js:babel-core/register --require ignore-styles --require mocha-clean && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HBOCodeLabs/<%=appname%>.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "griddle-react": "0.7.0",
    "history": "4.4.0",
    "lodash": "4.16.6",
    "material-design-icons": "3.0.1",
    "minimal-react-redux-notify": "1.0.12",
    "react": "15.3.2",
    "react-checkbox-group": "3.1.0",
    "react-dom": "15.3.2",
    "react-grid-layout": "0.13.9",
    "react-player-controls": "0.5.9",
    "react-redux": "4.4.5",
    "react-redux-notify": "1.0.8",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-devtools-extension": "1.0.0",
    "redux-observable": "0.12.2",
    "redux-persist": "4.0.0-alpha7",
    "rxjs": "5.0.0-rc.4"
  },
  "devDependencies": {
    "archiver": "1.2.0",
    "autoprefixer": "6.6.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.9.1",
    "babel-eslint": "7.1.0",
    "babel-istanbul": "0.12.2",
    "babel-istanbul-loader": "0.1.0",
    "babel-loader": "6.2.7",
